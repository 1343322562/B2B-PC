.settle{background-color:#fff;-webkit-user-select:none;}
.settle-content{min-height:600px;margin:20px auto;}
.settle-content__title{height:40px;line-height:40px;font-size:15px;color:#555;font-weight:550;}
.settle-content__title>.discount{font-size: 10px;font-weight: normal;padding: 2px 6px;background-color: red;color: #fff;border-radius: 10px;margin-left: 10px;}
.settle-content__address{height:50px;line-height:50px;border:1px solid #ddd;color:#444;}
.settle-content__address>span{margin:0px 20px;}
.settle-content__payWayList,.settle-content__payWayList>li{border-bottom:1px solid #ddd;}
.settle-content__payWayList>li{margin-left:20px;position:relative;height:40px;line-height:40px;}
.settle-content__payWayList>li:last-child{border:none;}
.settle-content__payWayList>li>i{margin-right:10px;width:40px;height:40px;}
.settle-content__payWayList>li>div>i{height:23px;width:23px;border-radius:100%;background-position:100px 100px;border:1px solid #ddd;margin-left:10px;cursor:pointer;}
.settle-content__payWayList>li>div>i.act{background-position:-211px -11px;border-color:#fff;}
.settle-content__payWayList>li>div{position:absolute;top:0;right:0;color:#999;}
.settle-content__payWayList>li>i,.settle-content__payWayList>li>div>i{display:inline-block;vertical-align:middle;background-image:url("../../images/settelIcon.png");background-repeat:no-repeat;}
.settle-content__payWayList--ye>i{background-position:-84px -5px;}
.settle-content__payWayList--zfb>i{background-position:-5px -5px;}
.settle-content__payWayList--wx>i{background-position:-45px -5px;}
.settle-content__payWayList--hdfk>i{background-position:-124px -5px;}
.settle-content__goodsListTitle,.settle-content__goodsList{width:98%;margin:0 auto;padding-left:2%;}
.settle-content__goodsList{border-bottom:1px solid #ddd;}
.settle-content__goodsListTitle>li{display:inline-block;border-bottom:1px solid #ddd;height:40px;line-height:40px;color:#888;}
.settle-content__goodsList>li>span{display:inline-block;height:40px;line-height:40px;}
.settle-content__goodsList>li>span .y-price{color:#999;text-decoration: line-through;margin-left:5px;}
.settle-content__orderTitle{height:40px;line-height:40px;padding-left:20px;text-align: right;}
.settle-content__orderTitle>span:last-child{float:right;color:red;display: inline-block;width: 120px;}
.settle-content__orderTitle .textB{font-size: 23px;}
.settle-content__goodsList .li5{color:red;}
.settle-content__list{text-align:center;}
.settle-content__list .li0{width:30%;text-align:left;}
.settle-content__list .gift{color: red;}
.settle-content__list .li1{width:20%;}
.settle-content__list .li2{width:15%;}
.settle-content__list .li3{width:10%;}
.settle-content__list .li4{width:10%;}
.settle-content__list .li5{width:15%;text-align:right;}
.settle-content__remarks{resize:none;outline:none;width:98%;height:90px;margin:0 auto;display:block;font-size:17px;}
.settle-footer{background-color:#e9e9e9;height:50px;line-height:50px;text-align:right;margin:50px 0;}
.settle-footer>div{margin:0 auto;font-size:17px;}
.settle-footer i{font-size:1.2em;}
.settle-footer>div>a{display:inline-block;color:#fff;background-color:red;font-size:18px;padding:0 30px;margin-left:10px;}
.settle-promotion__box{border:2px solid #ddd;position: relative}
.settle-promotion__title{background-color:#f6f6f6;margin-bottom:10px;display: flex;}
.settle-promotion__title>li{position: relative;height:37px;line-height:37px;padding:0 20px;font-size:12px;font-weight:700;color: #313131;cursor:pointer;-webkit-user-select: none;}
.settle-promotion__title>li.act{color:#e43a3d;background-color:#fff;}
.settle-promotion__title>li .num{font-weight: normal;text-align: center;font-size: .5em;color: #fff;background-color: #df0011;border-radius: 10px;top: -5px;right: 6px;position: absolute;height: 20px;line-height: 20px;padding: 0 3px;min-width: 16px;}
/*优惠券*/
.settle-promotion__coupons{overflow:hidden;width:1180px;margin:0 auto;}
.settle-promotion__coupons>li{float:left;background-color:#e70012;border:1px solid #e70012;width:172px;margin:10px;cursor:pointer;position:relative;}
.settle-promotion__coupons>li>p{padding:0 10px;}
.settle-promotion__coupons>li:before{position:absolute;content:"";top:-4px;left:0;height:5px;width:100%;background:url('../../images/settle-coupons-top.png')repeat-x 2px -6px;background-size:12px;}
.settle-promotion__coupons>li.act:after{content:"使用";border:1px solid #fff;font-size: 10px;position: absolute;top: 7px;right: 0;color: #fff;border-radius: 10px;width: 40px;height: 22px;line-height: 22px;text-align: center;transform: rotate(36deg);}
.settle-promotion__coupons .price{color:#fff;font-size:13px;margin:2px 0px;}
.settle-promotion__coupons .price>span:first-child{font-size:1.8em;margin-right:5px;}
.settle-promotion__coupons .date{color:#fff;font-size:10px;margin-bottom:10px;}
.settle-promotion__coupons .useInfo{background-color:#fff;height:30px;line-height:30px;font-size:12px;color:#666;}
.settle-promotion__coupons .useInfo>i{display:inline-block;float:right;height:100%;width:20px;background:url('../../images/settel-eye.png')no-repeat center center;background-size:100%;}
.settle-promotion__coupons--clear{position: absolute;right: 10px;top: 6px;padding: 3px 10px;border-radius: 10px;border: 1px solid #999;cursor: pointer;font-size: 10px;}
.settle-promotion__mzAndMj{padding:10px;}
.settle-promotion__mzAndMj .mjmzTitle{color:#ff5253;font-size:14px;}
/* .settle-promotion__mzAndMj .mjmzTitle>i{display:inline-block;background:url('../../images/settelIcon.png')no-repeat;width:40px;height:27px;vertical-align:middle;background-size:auto 40px;} */
.settle-promotion__mzAndMj .mjmzTitle>i{display:inline-block;    padding: 3px 6px;margin-top: 4px;margin-right: 5px;border-radius: 5px;font-size: 13px;background-color: #ff5253;color: #fff;}
.settle-promotion__mzAndMj .mj>i{background-position: -262px -5px;}
.settle-promotion__mzAndMj .mz>i{background-position: -305px -5px;}
.settle-promotion__mz_title{margin-top:10px;}
.settle-promotion__mz_title>.act{background-color:#fff;position:relative;height:31px;margin-bottom:-1px;z-index:10;border-bottom-color:#fff;}
.settle-promotion__mz_title>li{display:inline-block;padding:0 10px;height:30px;line-height:30px;border-radius:10px 10px 0 0;color:#777;cursor:pointer;-ms-user-select:none;user-select:none;background-color:#f6f6f6;border:1px solid #ddd;margin-right:5px;}
.settle-promotion__mz_title>li>i{display:inline-block;color:#fff;background-color:#f9a21d;line-height:20px;font-size:12px;border-radius:5px;padding:0 5px;margin-left:5px;}
.settle-promotion__mz_title>li>.act{background-color:#11D08B;}
.settle-promotion__mz_box{border:1px solid #ddd;padding:10px;position: relative;}
.settle-promotion__mz_Null{position: absolute;right: 0;top: -50px;padding: 5px 10px;border-radius: 10px;border: 1px solid #999;cursor: pointer;}
.settle-promotion__mz_box>.msg{height:40px;line-height:40px;}
.settle-promotion__mz_Group{overflow:hidden;}
.settle-promotion__mz_Group>li{float:left;text-align:center;padding:10px 15px;border:1px solid #ddd;background-color:#f9f9f9;margin:0 10px 10px 0;cursor:pointer;}
.settle-promotion__mz_Group>.act{background-color:#e70012;border-color:#e70012;color:#fff;}
.settle-promotion__mz_Group>li>span{display:block;}
.settle-promotion__mz_Group>li>span:last-child{font-size:.9em;}
.settle-promotion__mz_goodsList{border:1px solid #ddd;}
.settle-promotion__mz_goodsList>.title{background-color:#f3f3f3;}
.settle-promotion__mz_goodsList span{display:inline-block;height:35px;line-height:35px;text-align:center;}
.settle-promotion__mz_goodsList .li0{text-align:left;width:35%;margin-left:2%;}
.settle-promotion__mz_goodsList .li1{width:20%;}
.settle-promotion__mz_goodsList .li2{width:18%;}
.settle-promotion__mz_goodsList .li3{width:20%;}
.settle-promotion__mz_goodsList .li4{width:20%;}

.settle-promotion__DHcoupons{overflow:hidden;padding:10px 20px;}
.settle-promotion__DHcoupons>li{float:left;position:relative;width:221px;margin:15px 5px 0 5px;}
.settle-promotion__DHcoupons .name{font-size:15px;margin-bottom:5px;height:42px;overflow:hidden;}
.settle-promotion__DHcoupons .time{font-size:13px;}
.settle-promotion__DHcoupons .info{background-color:#eb5b13;color:#fff;padding:5px 10px;}
.settle-promotion__DHcoupons .btn{position:absolute;bottom:0;right:0;display:block;height:30px;width:90px;text-align:center;line-height:30px;}

.settle-promotion__DHcoupons .btn>i{
	display:block;position:absolute;height:30px;width:30px;top:0;background:url('../../images/addAndMinus.png')no-repeat;background-size:80%;cursor:pointer;
}
.settle-promotion__DHcoupons .btn>i:first-child{left:0;background-position:center 3px;}
.settle-promotion__DHcoupons .btn>i:last-child{right:0;background-position:center -25px;}
.settle-promotion__DHcoupons .msg{height:30px;line-height:30px;font-size:12px;padding-left:10px;border: 1px solid #eb5b13;}
.settle-promotion__DHcoupons .use{color:#fff;background-color:#eb5b13;display:block;position:absolute;right:10px;bottom:4px;height:26px;line-height:26px;padding:0 10px;border-radius:5px;cursor:pointer;}
.settle-promotion__DHcoupons>li:before{content:"";position:absolute;top:-7px;left:0;height:7px;;width:100%;background:url('../../images/settle-coupons-top.png')repeat-x -3px -14px;background-size:15px;}
.settle-promotion__DHcoupons_submit{height:40px;line-height:40px;margin:10px;background:#f6f6f6;}
.settle-promotion__DHcoupons_submit>a{display:inline-block;float:right;width:100px;text-align:center;background-color:#df0011;color:#fff;}
.settle-promotion__DHcoupons_submit>.act{background-color:#999;}

.settle-promotion__msg{color:red;margin-left:10px;font-size:14px;}

.order-success{
	width: 800px;
	margin: 60px auto;
	padding-top: 100px;
	background: url("../../images/success_icon.png")no-repeat center top;
	background-size: 60px;
	min-height: 600px;
}
.order-success>.title{
	font-size: 26px;
	border-bottom: 1px solid #e8e8e8;
	text-align: center;
	padding-bottom: 20px;
}
.order-success>.cnt{color: #777;padding-top: 20px;}
.order-success>.btn{
	text-align: center;
	padding-top: 60px;
}
.order-success>.btn a{
	display: inline-block;
	width: 160px;
	height: 30px;
	line-height: 30px;
	color: #fff;
	background: #e70012;
	margin: 0 20px;
	border-radius: 5px;
}
